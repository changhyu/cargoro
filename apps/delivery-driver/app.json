{
  "expo": {
    "name": "CarGoro 탁송 기사",
    "slug": "cargoro-delivery-driver",
    "version": "0.1.0",
    "main": "index.js",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cargoro.deliverydriver"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.cargoro.deliverydriver"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-location", "expo-image-picker"],
    "scheme": "cargoro-delivery",
    "platforms": ["ios", "android", "web"]
  }
}
