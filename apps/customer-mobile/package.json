{
  "name": "@cargoro/customer-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "NODE_OPTIONS='--no-deprecation' expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "NODE_OPTIONS='--no-deprecation' expo start --web --port 19001",
    "build": "NODE_OPTIONS='--no-deprecation' expo export:web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .expo node_modules/.cache",
    "start:clean": "npm run clean && NODE_OPTIONS='--no-deprecation' expo start --port 19001"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.1",
    "@cargoro/api-client": "workspace:*",
    "@cargoro/gps-obd-lib": "workspace:^",
    "@cargoro/types": "workspace:*",
    "@cargoro/utils": "workspace:*",
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^18.0.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-navigation/bottom-tabs": "6.5.11",
    "@react-navigation/native": "6.1.10",
    "@react-navigation/native-stack": "6.9.17",
    "@react-navigation/stack": "6.3.21",
    "@tanstack/react-query": "^5.0.0",
    "@types/react-dom": "18.0.11",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "expo": "~48.0.21",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-device": "~5.2.1",
    "expo-haptics": "~12.2.1",
    "expo-image-picker": "~14.1.1",
    "expo-linking": "~4.0.1",
    "expo-location": "~15.1.1",
    "expo-notifications": "~0.18.1",
    "expo-secure-store": "~12.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.0",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "lottie-react-native": "5.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.14",
    "react-native-gesture-handler": "2.9.0",
    "react-native-maps": "1.3.2",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.15.0",
    "react-native-svg": "13.4.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.18.10",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@testing-library/react-native": "^12.0.0",
    "@types/react": "18.0.38",
    "@types/react-native": "~0.71.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "typescript": "5.4.5",
    "vitest": "^1.0.0"
