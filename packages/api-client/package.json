{
  "name": "@cargoro/api-client",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit --skipLibCheck --project ./tsconfig.json",
    "build": "tsup index.ts",
    "dev": "tsup index.ts --watch",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "build:dev": "tsc --noEmit --project ../../tsconfig.build.json"
  },
  "dependencies": {
    "@cargoro/types": "workspace:*",
    "@cargoro/utils": "workspace:*",
    "@clerk/nextjs": "^6.20.2",
    "@react-navigation/native-stack": "^7.3.13",
    "@tanstack/react-query": "^5.75.8",
    "@tanstack/react-query-devtools": "^5.75.8",
    "axios": "^1.7.7",
    "graphql": "^16.9.0",
    "graphql-request": "^6.1.0",
    "next": "14.2.25",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/client-preset": "^4.4.0",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.0",
    "@graphql-codegen/typescript-operations": "^4.3.0",
    "@graphql-codegen/typescript-react-query": "^6.1.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^20.3.2",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^8.44.0",
    "jsdom": "^24.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^3.1.4"
  }
}
